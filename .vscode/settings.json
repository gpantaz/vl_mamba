{
  "files.exclude": {
	"**/.git": true,
	"**/.svn": true,
	"**/.hg": true,
	"**/CVS": true,
	"**/.DS_Store": true,
	"**/Thumbs.db": true,
	"**/.idea": true,
	"**/LICENSE": true,
	"**/.coverage*": true,
	"**/.hypothesis": true,
	"**/{*.egg-info,*cache*,.venv,*.lock,.pyre}": true,
	"**/{.python-version,py.typed}": true,
	"**/.gitkeep": true,
	"**/.gitignore": true,
	"**/.gitattributes": true,
	".storage": true,
	".editorconfig": false,
	".pre-commit-config.yaml": false,
	".mypy.ini": true,
	".flake8": true,
	".dockerignore": true,
	"Dockerfile": true,
	"docker-compose.yaml": true,
	"Makefile": true,
	".env*": true,
	".github/": true,
	".vscode/": true,
	"*.md": true,
	"data": true,
	"poetry.toml": false,
	".cov.xml": true,
	"**/ruff.toml": true,
	".pdm-python": true,
	".pdm-build": true,
	".pdm-plugins": true,
	"wandb": true,
	"**/node_modules": true,
	"node_modules": true
},
  "explorerExclude.backup": {},
  "editor.wordWrapColumn": 99,
  "editor.rulers": [99],
  "git.ignoreLimitWarning": true,
  "[json,yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "notebook.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "[python]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": "always",
      "source.organizeImports": "always"
    },
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "python.testing.pytestEnabled": true,
  "python.testing.cwd": "${workspaceFolder}",
  "shellformat.useEditorConfig": true,
  "rewrap.autoWrap.enabled": true,
  "editor.insertSpaces": false,
  "autoDocstring.docstringFormat": "google-notypes",
  "python.testing.pytestArgs": ["tests"],
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "python.testing.unittestEnabled": false,
  "python.languageServer": "None",
  "evenBetterToml.semanticTokens": true,
  "evenBetterToml.schema.links": true,
  "evenBetterToml.formatter.compactArrays": true,
  "evenBetterToml.formatter.compactInlineTables": true,
"basedpyright.analysis.diagnosticMode": "workspace",
"basedpyright.disableTaggedHints": true,
"editor.inlayHints.enabled": "off"
}
